#LC704二分查找
#题目链接：https://leetcode.cn/problems/binary-search/submissions/651712244/

class Solution:
     def search(self,nums,target):
         left=0
         right=len(nums)-1
         while(left<=right):
             middle=(left+right)//2
             if (nums[middle]>target):
                 right=middle-1
             elif(nums[middle]<target):
                 left=middle+1
             else:
                 return middle
         return -1
